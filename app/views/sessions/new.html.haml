- content_for :landing do
  .container
    .pad-top
      .row
        .col-lg-4.col-md-4.col-sm-12.col-xs-12.col-lg-offset-4
          .navbar-default.text-center.pad-top.pad-bottom
            = render 'layouts/branding'
      .row
        .col-lg-4.col-md-4.col-sm-12.col-xs-12.col-lg-offset-4
          .panel
            .form
              %h1.text-center Log In
              = form_for :session, url: login_path do |f|
                .form-group
                  = f.email_field :email, placeholder: :email, class: "form-control"
                .form-group
                  = f.password_field :password, placeholder: :password, class: "form-control"
                .form-group
                  = f.submit "Sign In", class: "btn btn-primary btn-lg btn-block"
          %p
            Not a member yet?
            %span= link_to "Sign Up", signup_path
