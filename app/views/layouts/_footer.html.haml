          %li.item
            -# = link_to this_is_label, this_is_link_path, this_is_link_title
            = link_to 'Wishlist', "#", title: "Wishlist"
          %li.item
            = link_to "Your Account", "#", title: "Your Account"
          %li.item
            = link_to "Checkout", "#", title: "Check"
          %li.item
            = link_to "Login", "#", title: "Login"
          %li.item
            = link_to "Register", title: "Register"
      .col-md-3.col-sm-6.col-xs-12
