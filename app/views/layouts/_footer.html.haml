        %h4.title_block Service & Support
        %ul.list-group.list-unstyled
          %li.item
            = link_to "About Us", "#", title: "About Us"
          %li.item
            = link_to "Jobs", "#", title: "Jobs"
          %li.item
            = link_to "Affiliates", "#", title: "Affiliates"
          %li.item
            = link_to "Contact", "#", title: "Contact"
          %li.item
            = link_to "Blog", "#", title: "Blog"
      .col-md-3.col-sm-6.col-xs-12
        %h4.title_block About Store
        .footer_contact
          %strong Warehouse & Offices
          %p
            12345 Street name, California, USA
            %br>/
            0123 456 789 / 0123 456 788
          %strong Retail Store
          %p
            54321 Street name, California, USA
            %br>/
            0123 456 789 / 0123 456 788
